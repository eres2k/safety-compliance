{
  "name": "whs-safety-navigator",
  "version": "1.0.0",
  "type": "module",
  "description": "Workplace Health and Safety Compliance Navigator for Austria, Germany, and Netherlands",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext js,jsx",
    "clean:at": "node scripts/clean-at-database.js",
    "clean:de": "node scripts/clean-de-database.js",
    "clean:nl": "node scripts/clean-nl-database.js",
    "clean:all": "npm run clean:at && npm run clean:de && npm run clean:nl",
    "tag:semantic": "node scripts/generate-semantic-tags.js",
    "tag:semantic:at": "node scripts/generate-semantic-tags.js --country=at",
    "tag:semantic:de": "node scripts/generate-semantic-tags.js --country=de",
    "tag:semantic:nl": "node scripts/generate-semantic-tags.js --country=nl",
    "tag:semantic:index": "node scripts/generate-semantic-tags.js --index"
  },
  "dependencies": {
    "@netlify/blobs": "^8.1.0",
    "mermaid": "^11.12.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.4"
  }
}
